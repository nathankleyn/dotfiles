{
  "telemetry.enableTelemetry": false,
  "editor.fontFamily": "SF Mono",
  "editor.tabSize": 2,
  "[rust]": {
    "editor.tabSize": 4,
  },
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "workbench.startupEditor": "newUntitledFile",
  "editor.rulers": [
    80,
    100,
    120
  ],
  "editor.multiCursorModifier": "ctrlCmd",
  "markdown.preview.scrollEditorWithPreview": false,
  "markdown.preview.scrollPreviewWithEditor": false,
  "latex-workshop.view.pdf.viewer": "tab",
  "terminal.integrated.shell.osx": "/opt/homebrew/bin/fish",
  "shellcheck.customArgs": [
    "-x"
  ],
  "editor.renderWhitespace": "none",
  "files.associations": {
    "*.nomad": "terraform"
  },
  "todo-tree.tree.showScanModeButton": false,
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "shellcheck.executablePath": "/opt/homebrew/bin/shellcheck",
  "editor.fontLigatures": false,
  "editor.fontSize": 14,
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "window.commandCenter": false,
  "aws.telemetry": false,
  "editor.accessibilitySupport": "off",
  "window.nativeTabs": true,
  "stripe.cliInstallPath": "/opt/homebrew/bin/stripe",
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "kotlin.languageServer.enabled": false,
  "kotlin.debugAdapter.enabled": false,
  "workbench.layoutControl.enabled": false,
  "debug.javascript.autoAttachFilter": "onlyWithFlag",
  "prisma.showPrismaDataPlatformNotification": false,
  "[vue]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "jupyter.askForKernelRestart": false,
  "workbench.iconTheme": "catppuccin-frappe",
  "workbench.colorTheme": "Catppuccin FrappÃ©",
  "vue.server.hybridMode": true,
  "zig.zls.path": "zls",
  "playwright.showTrace": false,
  "github.copilot.nextEditSuggestions.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "javascript": false,
    "vue": true,
    "ini": false,
    "typescript": true,
    "html": false
  },
  "spellright.groupDictionaries": false,
  "spellright.language": [
    "en_US",
    "en_GB",
  ],
  "spellright.languageContext": {
    "strings": [ "en_US", "en_GB" ],
    "comments": [ "en_US", "en_GB" ]
  },
  "docker.extension.dockerEngineAvailabilityPrompt": false
}
